/dts-v1/;
#include "ma35d1.dtsi"

/ {
	model = "Nuvoton MA35D1 EVB board";
	compatible = "nuvoton,ma35d1";

	aliases {

	};

	ddr: ddr-controller {
		compatible = "issi-ddr3-1gb";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x80000000 0 0x1e000000>;
	};

};

&uart0 {
	status = "okay";
};





